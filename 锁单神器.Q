[General]
SyntaxVersion=2
BeginHotkey=0
BeginHotkeyMod=0
PauseHotkey=0
PauseHotkeyMod=0
StopHotkey=123
StopHotkeyMod=0
RunOnce=1
EnableWindow=
MacroID=404b0eeb-7057-4c09-8adc-ed11f0834b83
Description=锁单神器
Enable=1
AutoRun=0
[Repeat]
Type=0
Number=1
[SetupUI]
Type=1
QUI=Form1
[Relative]
SetupOCXFile=
[Comment]

[UIPackage]



[Script]

Event Form1.Button1.Click   
    If Form1.BrowseBox1.Path <> "" Then  
        login = Form1.BrowseBox1.Path
        Else 
        Form1.Label3.Caption = "LOG：错误，请输入登录文件路径"
        Form1.Label3.TextColor = "0000FF" 
        Goto 退出
    End If
    
    If Form1.BrowseBox2.Path <> "" Then 
    	things = Form1.BrowseBox2.Path
    	Else 
    	Form1.Label3.Caption = "LOG：错误，请输入信息文件路径"
    	Form1.Label3.TextColor = "0000FF"
    	Goto 退出
    End If
    
    Login = Plugin.File.OpenFile(login)
    
    If Login = -1 Then
    	Form1.Label3.Caption = "LOG：错误，无法打开登陆文件，请检查"
    	Form1.Label3.TextColor = "0000FF"
    	Goto 退出
    	Else 
    	Form1.Label3.Caption = "LOG：打开登陆文件..."
    	Form1.Label3.TextColor = "008000" 
    End If
    
    Things = Plugin.File.OpenFile(Things)
    
    If Things = -1 Then
    	Form1.Label3.Caption = "LOG：错误，无法打开信息文件，请检查"
    	Form1.Label3.TextColor = "0000FF"
    	Goto 退出
    	Else 
    	Form1.Label3.Caption = "LOG：打开信息文件..."
    	Form1.Label3.TextColor = "008000" 
    End If
    
    ProcessID=Plugin.Web.Bind("wqm.exe")
	Delay 1000
	If ProcessID=0 then
  		Form1.Label3.Caption = "LOG：网页按键精灵没有运行"
    	Form1.Label3.TextColor = "0000FF"
    	Goto 退出
    	Else 
    	Form1.Label3.Caption = "LOG：打开网页按键精灵..."
    	Form1.Label3.TextColor = "008000" 
	End If
	
	logintext = Plugin.File.ReadLine(Login)
	thingstext = Plugin.File.ReadLine(Things)
	
	Do While logintext <> ""
		loginarray = Split(logintext,",")
		Call Plugin.Web.Go("http://ifeidai.com/EDaiWeb/AgentLoan/Login")
		Call Plugin.Web.HtmlInput(loginarray(0),"id:txtUserName")
		Call Plugin.Web.HtmlInput(loginarray(1), "id:txtPwd")
		Delay 1000
		ident = Plugin.Web.HtmlGet("value", "id:ValidateValue")
		Delay 1000
		Call Plugin.Web.HtmlInput(ident, "id:txtVerification")
		Delay 1000
		Call Plugin.Web.HtmlClick("id:btnSave")
		Delay 1000
		Call Plugin.Web.Go("http://ifeidai.com/EDaiWeb/AgentLogon/MySaleApplyBill?Length=10")
		Delay 1000
		Call Plugin.Web.HtmlClick("id:FastRecommend")
    	Exit Do
    	//退出循环
	Loop

    Plugin.File.ReadLine(Login)

    
    Rem 退出
    
	Call Plugin.File.CloseFile(Login)
	Call Plugin.File.CloseFile(Things)
End Event 
